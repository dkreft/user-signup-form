{
  "name": "auth-form",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx",
    "start": "next start",
    "test": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "next": "9.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "bdd-lazy-var": "^2.5.4",
    "classnames": "^2.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-perf": "^3.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.14.1"
  },
  "jest": {
    "clearMocks": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/.*index.js",
      "<rootDir>/test/"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|svg)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupJest.js"
    ]
  }
}
